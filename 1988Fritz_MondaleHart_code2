function switcheroo() {
    campaignTrail_temp.running_mate_image_url = 'https://upload.wikimedia.org/wikipedia/commons/thumb/f/f8/Bob_Graham_at_press_conference.jpg/180px-Bob_Graham_at_press_conference.jpg';
    campaignTrail_temp.running_mate_last_name = 'Graham';
}

function tunnel(new_q){
    e = campaignTrail_temp;
    return e.questions_json[e.questions_json.map(f=>f.pk).indexOf(new_q)];
}

cyoAdventure = function(a) {
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
    if(ans == 15652){
        console.log("done");
        switcheroo();
 }
}


